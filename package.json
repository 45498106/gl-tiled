{
  "name": "gl-tiled",
  "version": "1.0.0",
  "main": "./dist/gl-tiled.js",
  "description": "A Tiled editor renderer for WebGL.",
  "author": "Chad Engler <chad@pantherdev.com>",
  "license": "MIT",
  "homepage": "https://github.com/englercj/gl-tiled",
  "repository": {
    "type": "git",
    "url": "https://github.com/englercj/gl-tiled.git"
  },
  "bugs": {
    "url": "https://github.com/englercj/gl-tiled/issues"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production rollup -c",
    "dev": "rollup -c -w",
    "docs": "typedoc --includeDeclarations --excludeExternals --excludePrivate --mode modules --module commonjs --target ES5 --out docs/ ./src ./bundles",
    "prepublish": "npm run build",
    "postpublish": "npm run deploy",
    "predeploy": "rimraf ./docs && npm run docs && touch ./docs/.nojekyll",
    "deploy": "gh-pages --dotfiles -d docs"
  },
  "keywords": [
    "gl",
    "webgl",
    "html5",
    "tilemap",
    "tile",
    "tiled",
    "map",
    "render",
    "2d"
  ],
  "files": [
    "dist/",
    "LICENSE",
    "README.md",
    "package.json"
  ],
  "dependencies": {
    "cross-env": "^5.1.1",
    "gl-matrix": "^2.4.0"
  },
  "devDependencies": {
    "@types/gl-matrix": "^2.4.0",
    "gh-pages": "^1.0.0",
    "resource-loader": "^2.0.9",
    "rimraf": "^2.6.2",
    "rollup": "^0.50.1",
    "rollup-plugin-commonjs": "^8.2.6",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-preprocess": "^0.0.4",
    "rollup-plugin-string": "^2.0.2",
    "rollup-plugin-typescript2": "^0.8.0",
    "stats.js": "^0.17.0",
    "typedoc": "^0.9.0",
    "typedoc-md-theme": "^1.0.1",
    "typescript": "2.5.3"
  }
}
